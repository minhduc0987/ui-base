{
  "name": "@base-ui-t1/components",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/YOUR_REPOSITORY_NAME"
  },
  "version": "1.0.0",
  "description": "Base UI component for T1 project",
  "scripts": {
    "build-dev": "./node_modules/.bin/rimraf dist/ && cross-env NODE_ENV=development rollup -c",
    "build-prod": "./node_modules/.bin/rimraf dist/ && cross-env NODE_ENV=production rollup -c",
    "bump-version-major": "npm version major --no-git-tag-version",
    "bump-version-minor": "npm version minor --no-git-tag-version",
    "bump-version-patch": "npm version patch --no-git-tag-version",
    "pack": "npm pack",
    "publish": "sh ./publish-release.sh",
    "clean": "./node_modules/.bin/rimraf dist/",
    "lint": "./node_modules/.bin/eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "./node_modules/.bin/eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
    "lint:style": "./node_modules/.bin/stylelint src/**/*.scss",
    "lint:style:fix": "./node_modules/.bin/stylelint **/*.scss --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "./node_modules/.bin/eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
      "npm run format",
      "git add ."
    ],
    "**/*.scss": [
      "./node_modules/.bin/stylelint '**/*.scss' --fix",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/.bin/lint-staged"
    }
  },
  "author": "Niteco",
  "license": "ISC",
  "devDependencies": {
